<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<validators xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="http://10.10.10.141/easyframe/schema/validators.xsd">
	<validator id="com.ailk.phonenumber" regexp="^1[3,5,8]{1}[0-9]{1}[0-9]{8}$"/>
	<validator id="com.ailk.notempty" regexp=".{1,}"/>
	<validator id="com.ailk.email" regexp="^|(\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*)$"/>
	<validator id="com.ailk.url" regexp="[a-zA-z]+://[^\s]*"/>
	<validator id="com.ailk.userAccount" regexp="^[a-zA-Z][a-zA-Z0-9_]{4,15}$"/>
	<validator id="com.ailk.landPhone" regexp="\d{3}-\d{8}|\d{4}-\d{7}"/>
	<validator id="com.ailk.chinaPostCode" regexp="[1-9]\d{5}(?!\d)"/>
	<validator id="com.ailk.chinaIdCard" regexp="\d{15}|\d{18}"/>
	<validator id="com.ailk.ipAddr" regexp="^\d+\.\d+\.\d+\.\d+$"/>
	<validator id="com.ailk.userAccount" regexp="^[a-zA-Z][a-zA-Z0-9_]{4,15}$"/>
	<validator id="com.ailk.chineseChar" regexp="\u4e00-\u9fa5"/>
</validators>